{
    "name": "react-sales-pilot-chrome-extension",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react-scripts": "5.0.1",
        "socket.io-client": "^4.6.1",
        "webpack": "^5.75.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build:unpacked": "npx webpack && cd ./front-end && npm run build:unpacked && cd ..",
        "build:packed": "npx webpack && cd ./front-end && npm run build:packed && cd ../ && cp -Rf ./dist ./react ./sidebar ./bg-wrapper.js ./manifest.json ./packed/ ./public/",
        "build": "npm run build:unpacked",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
